{
  "name": "storybook-jira-addon",
  "version": "0.0.9",
  "description": "An extra panel to show JIRA tickets concerning certain components.",
  "keywords": [
    "storybook-addons",
    "tickets",
    "JIRA",
    "Atlassian"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lennartdeknikker/storybook-addon-jira"
  },
  "author": "Lennart de Knikker",
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "buildBabel": "babel ./src --out-dir ./dist --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist",
    "build": "concurrently \"npm run buildBabel\" \"npm run buildTsc\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6007",
    "dev": "concurrently \"npm run storybook -- --no-manager-cache --quiet\" \"npm run build -- --watch\"",
    "build-storybook": "build-storybook",
    "prerelease": "node check-metadata.js",
    "release": "npm run build && auto shipit"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/react": "^6.3.0",
    "auto": "^10.3.0",
    "babel-loader": "^8.1.0",
    "chalk": "^2.4.2",
    "concurrently": "^5.3.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.3.0",
    "@storybook/api": "^6.3.0",
    "@storybook/components": "^6.3.0",
    "@storybook/core-events": "^6.3.0",
    "@storybook/theming": "^6.3.0",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "nano-markdown": "^1.2.1"
  }
}
